        "def collect_called_functions(body):\n",
        "    called = set()\n",
        "    class CallVisitor(ast.NodeVisitor):\n",
        "        def visit_Call(self, node):\n",
        "            if isinstance(node.func, ast.Name):\n",
        "                called.add(node.func.id)\n",
        "            self.generic_visit(node)\n",
        "    for stmt in body:\n",
        "        CallVisitor().visit(stmt)\n",
        "    return called\n",
        "\n",
        "def build_adjacency_matrix(functions):\n",
        "    name_to_index = {fn.name: i for i, fn in enumerate(functions)}\n",
        "    n = len(functions)\n",
        "    matrix = [[0]*n for _ in range(n)]\n",
        "    for i, fn in enumerate(functions):\n",
        "        if not fn.body: continue\n",
        "        called = collect_called_functions(fn.body)\n",
        "        for callee in called:\n",
        "            j = name_to_index.get(callee)\n",
        "            if j is not None:\n",
        "                matrix[i][j] = 1\n",
        "    return matrix\n"
      ]
    },
