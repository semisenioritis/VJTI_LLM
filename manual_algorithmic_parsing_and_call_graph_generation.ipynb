        "        self.name = name\n",
        "        self.lineno = lineno\n",
        "        self.args = []\n",
        "        self.docstring = None\n",
        "        self.decorators = []\n",
        "        self.is_async = False\n",
        "        self.body = None\n",
        "\n",
        "    def __repr__(self):\n",
        "        return f\"<FunctionInfo name={self.name} lineno={self.lineno}>\"\n",
        "\n",
        "def extract_functions(source_code):\n",
        "    tree = ast.parse(source_code)\n",
        "    functions = []\n",
        "    for node in ast.walk(tree):\n",
        "        if isinstance(node, (ast.FunctionDef, ast.AsyncFunctionDef)):\n",
