        "            if val:\n",
        "                dot.edge(node_ids[i], node_ids[j])\n",
        "\n",
        "    dot.render(output_file, view=True)\n",
        "\n",
        "visualize_function_call_graph(all_funcs, matrix)\n"
      ]
    }
